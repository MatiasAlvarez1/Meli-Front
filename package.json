  "license": "ISC",
  "scripts": {
    "dev": "NODE_ENV=development run-p -l dev:react dev:node ",
    "production": "NODE_ENV=production run-p -l build:client build:server start:node",
    "babel": "./node_modules/.bin/babel",
    "babel-node": "./node_modules/.bin/babel-node",
    "dev:react": "cd client && react-scripts start",
    "dev:node": "NODE_ENV=development babel-node ./server/index.js",
    "start:node": "babel-node ./server/index.js",
    "build:client": "cd client && react-scripts build",
    "build:server": "babel ./server --out-dir ./server/dist",
    "test": "cd client && react-scripts test",
    "eject": "cd client && react-scripts eject",
    "pre-commit": "npm test",
    "pre-push": "npm test"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
